<style>
    body {
      margin: 0;
      padding: 0;
      font-family: Arial, sans-serif;
      display: flex;
    }
    
    /* Sidebar Styles */
    .sidebar {
      margin-top: 55px;
      background-color: #f1f1f1;
      width: 250px;
      padding: 10px;
      overflow-y: scroll;
      height: calc(100vh - 20px);
    }
    
    .sidebar h2 {
      text-align: center;
      font-size: 18px;
      margin-top: 0;
    }
    
    .sidebar ul {
      list-style: none;
      margin: 0;
      padding: 0;
    }
    
    .sidebar li {
      padding: 10px 0;
      border-bottom: 1px solid #ddd;
    }
    
    .sidebar a {
      text-decoration: none;
      color: #333;
      display: block;
      padding: 5px;
    }
    
    .sidebar a:hover {
      color: #000;
      background-color: #e0e0e0;
    }
    
    /* Content Area Styles */
    .content {
        margin-top: 100px;
      flex: 1;
      padding: 20px;
      background-color: #fff;
      border: 1px solid #ddd;
      box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
    }
    
    /* Responsive Layout */
    @media (max-width: 768px) {
      .sidebar {
        width: 100%;
        float: none;
      }
    }
    
    .main {
      margin-left: 250px;
    }
    </style>
    
